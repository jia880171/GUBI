<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".ProDashboardReal">

    <TextView
        android:id="@+id/text_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="time"
        android:textColor="#ffffff"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <at.grabner.circleprogress.CircleProgressView
        android:id="@+id/circleProgress_speed"
        android:layout_marginTop="30dp"
        android:layout_width="300dp"
        android:layout_height="300dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_time"
        />

    <TextView
        android:id="@+id/text_speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="speed"
        app:layout_constraintLeft_toLeftOf="@+id/circleProgress_speed"
        app:layout_constraintTop_toBottomOf="@+id/circleProgress_speed"
        />

    <ProgressBar
        android:id="@+id/progressBar_lat"
        style="?android:attr/progressBarStyleHorizontal"
        android:progressDrawable="@drawable/progressbar"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        app:layout_constraintLeft_toLeftOf="@id/circleProgress_speed"
        app:layout_constraintTop_toBottomOf="@+id/text_speed"/>

    <TextView
        android:id="@+id/text_lat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:text="lat"
        android:layout_centerHorizontal="true"
        app:layout_constraintLeft_toLeftOf="@id/progressBar_lat"
        app:layout_constraintTop_toBottomOf="@+id/progressBar_lat"
        />

    <ProgressBar
        android:id="@+id/progressBar_lng"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_marginTop="30dp"
        android:progressDrawable="@drawable/progressbar"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/circleProgress_speed"
        app:layout_constraintTop_toBottomOf="@+id/text_lat"
        />
    <TextView
        android:id="@+id/text_lng"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:text="lng"
        app:layout_constraintLeft_toLeftOf="@id/progressBar_lng"
        app:layout_constraintTop_toBottomOf="@+id/progressBar_lng"
        />


    <at.grabner.circleprogress.CircleProgressView
        android:id="@+id/circleProgress_lat"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="3000dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_lng"
        />

    <at.grabner.circleprogress.CircleProgressView
        android:id="@+id/circleProgress_lng"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerHorizontal="true"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/circleProgress_lat"
        />

    <TextView
        android:id="@+id/text_latLng"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="latLng"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/circleProgress_lng"
        />
    <TextView
        android:id="@+id/text_degree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="degree"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_lat"
        />








</android.support.constraint.ConstraintLayout>